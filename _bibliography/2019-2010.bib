@article{arganarazChemistryStructureNickeltungsten2012,
  title = {The Chemistry and Structure of Nickel-Tungsten Coatings Obtained by Pulse Galvanostatic Electrodeposition},
  author = {Argañaraz, M. P.Quiroga and Ribotta, S. B. and Folquer, M. E. and Zelaya, E. and Llorente, C. and Ramallo-López, J. M. and Benítez, G. and Rubert, A. and Gassa, L. M. and Vela, M. E. and Salvarezza, R. C.},
