@article{mojicapisciottiVitroVivoExperiments2014,
  title = {In Vitro and in Vivo Experiments with Iron Oxide Nanoparticles Functionalized with {{DEXTRAN}} or Polyethylene Glycol for Medical Applications: {{Magnetic}} Targeting},
  author = {Mojica Pisciotti, M. L. and Lima, E. and Vasquez Mansilla, M. and Tognoli, V. E. and Troiani, H. E. and Pasa, A. A. and Creczynski-Pasa, T. B. and Silva, A. H. and Gurman, P. and Colombo, L. and Goya, G. F. and Lamagna, A. and Zysler, R. D.},
  date = {2014},
  journaltitle = {Journal of Biomedical Materials Research - Part B Applied Biomaterials},
  volume = {102},
  number = {4},
  eprint = {24458920},
  eprinttype = {pmid},
  pages = {860--868},
  issn = {15524981},
  doi = {10.1002/jbm.b.33068},
  abstract = {In this research work, DEXTRAN- and polyethylene glycol (PEG)-coated iron-oxide superparamagnetic nanoparticles were synthetized and their cytotoxicity and biodistribution assessed. Well-crystalline hydrophobic Fe3 O4 SPIONs were formed by a thermal decomposition process with d = 18 nm and σ = 2 nm; finally, the character of SPIONs was changed to hydrophilic by a post-synthesis procedure with the functionalization of the SPIONs with PEG or DEXTRAN. The nanoparticles present high saturation magnetization and superparamagnetic behavior at room temperature, and the hydrodynamic diameters of DEXTRAN- and PEG-coated SPIONs were measured as 170 and 120 nm, respectively. PEG- and DEXTRAN-coated SPIONs have a Specific Power Absorption SPA of 320 and 400 W/g, respectively, in an ac magnetic field with amplitude of 13 kA/m and frequency of 256 kHz. In vitro studies using VERO and MDCK cell lineages were performed to study the cytotoxicity and cell uptake of the SPIONs. For both cell lineages, PEG- and DEXTRAN-coated nanoparticles presented high cell viability for concentrations as high as 200 μg/mL. In vivo studies were conducted using BALB/c mice inoculating the SPIONs intravenously and exposing them to the presence of an external magnet located over the tumour. It was observed that the amount of PEG-coated SPIONs in the tumor increased by up to 160\% when using the external permanent magnetic as opposed to those animals that were not exposed to the external magnetic field.},
  isbn = {1552-4981},
  keywords = {cell uptake,hyperthermia,magnetic nanoparticles,mice,SPIONS biocompatibility,SPIONs biodistribution,SPIONS quantification,superparamagnetism,tumors},
  file = {C:\Users\Julio\Zotero\storage\MH7YZQYB\10.1002@jbm.b.33068.pdf}
}

