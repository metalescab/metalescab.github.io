@article{Lavorato2016,
  title = {Exchange Bias and Surface Effects in Bimagnetic {{CoO-core}}/{{Co0}}.{{5Ni0}}.{{5Fe2 O4}} -Shell Nanoparticles},
  author = {Lavorato, Gabriel and Winkler, Elin and Ghirri, Alberto and Lima, Enio and Peddis, Davide and Troiani, Horacio E. and Fiorani, Dino and Agostinelli, Elisabetta and Rinaldi, Daniele and Zysler, Roberto D.},
