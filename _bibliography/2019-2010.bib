@article{Glucksberg2018,
  title = {Releasing Systems for Aerospace Industry Based upon {{Shape Memory Alloys}}: Characterization of Materials for Actuators},
  author = {Glücksberg, Andrés and Soul, Hugo and Yawny, Alejandro},
  date = {2018-07-19},
  journaltitle = {Matéria (Rio de Janeiro)},
