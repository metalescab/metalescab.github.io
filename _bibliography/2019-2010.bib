@article{azcarateSurfaceChemistryThiomalic2014a,
  title = {Surface Chemistry of Thiomalic Acid Adsorption on Planar Gold and Gold Nanoparticles},
  author = {Azcárate, Julio C. and Floridia Addato, María Alejandra and Rubert, Aldo and Corthey, Gastón and Kürten Moreno, Germán S. and Benítez, Guillermo and Zelaya, Eugenia and Salvarezza, Roberto C. and Fonticelli, Mariano H.},
