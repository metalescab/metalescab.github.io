@article{wolosiukSilverNanoparticlemesoporousOxide2014,
  title = {Silver Nanoparticle-Mesoporous Oxide Nanocomposite Thin Films: {{A}} Platform for Spatially Homogeneous {{SERS-active}} Substrates with Enhanced Stability},
  author = {Wolosiuk, Alejandro and Tognalli, Nicolás G. and Martínez, Eduardo D. and Granada, Mara and Fuertes, M. Cecilia and Troiani, Horacio and Bilmes, Sara A. and Fainstein, Alejandro and Soler-Illia, Galo J A A},
