@article{cuscuetaElectrochemicalEffectCarbon2012,
  title = {Electrochemical Effect of Carbon Nanospheres on an {{AB5alloy}}},
  author = {Cuscueta, D. J. and Corso, H. L. and Arenillas, A. and Martinez, P. S. and Ghilarducci, A. A. and Salva, H. R.},
