@article{cuscuetaInnerPressureCharacterization2011,
  title = {Inner Pressure Characterization of a Sealed Nickel-Metal Hydride Cell},
  author = {Cuscueta, D. J. and Salva, H. R. and Ghilarducci, A. A.},
  date = {2011},
  journaltitle = {Journal of Power Sources},
