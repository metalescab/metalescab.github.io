@article{nosedagrauSoluteClusteringBehavior2016,
  title = {Solute Clustering Behavior between {{293K}} and {{373K}} in a 6082 {{Aluminum}} Alloy},
  author = {Noseda Grau, V. and Cuniberti, A. and Tolley, A. and Castro Riglos, V. and Stipcich, M.},
  date = {2016-11},
