@article{Chanquia2012,
  title = {Synthesis and Characterization of Pure-Phase {{La0}}.{{75Sr0}}.{{25Cr0}}.{{5Mn0}}.{{5O3-δ}} Nanocrystallites for Solid Oxide Fuel Cell Applications},
  author = {Chanquía, Corina M. and Vega-Castillo, Jesús E. and Soldati, Analía L. and Troiani, Horacio and Caneiro, Alberto},
