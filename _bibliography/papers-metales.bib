  author = {Malachevsky, Maria Teresa and Bertolino, Graciela and Arneodo, Pierre and Baruj, Alberto and Oliber, Edgardo and D’Ovidio, Claudio and Cuscueta, Diego},
  date = {2018-07-19},
  journaltitle = {Matéria (Rio de Janeiro)},
