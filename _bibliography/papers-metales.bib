  keywords = {lattice parameters,metastable phases,omega phase,phase transformations,zirconium niobium alloys},
  file = {C:\Users\Julio\Zotero\storage\VNFR3KHU\Benites, Fernández Guillermet - 2000 - Structural properties of metastable phases in Zr-Nb alloys II. Systematics of the atomic volumes.pdf}
}

