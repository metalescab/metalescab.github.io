@article{Malachevsky2015,
  title = {Cubic {{Phases}} in the {{Gd}} 2 {{O}} 3 -{{ZrO}} 2 and {{Dy}} 2 {{O}} 3 -{{TiO}} 2 {{Systems}} for {{Nuclear Industry Applications}}},
