@article{benitesStructuralPropertiesMetastable2000,
  title = {Structural Properties of Metastable Phases in {{Zr-Nb}} Alloys {{I}}. {{Neutron}} Diffraction Study and Analysis of Lattice Parameters},
  author = {Benites, G.M. M and Fernández Guillermet, A. and Cuello, G.J. J and Campo, Javier and Guillermet, A F and Cuello, G.J. J and Campo, Javier},
  date = {2000-03-14},
  journaltitle = {Journal of Alloys and Compounds},
