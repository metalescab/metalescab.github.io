}

@article{sadeEffectGrainSize2014,
  title = {Effect of Grain Size on Stress Induced Martensitic Transformations in a {{Cu-Al-Be}} Polycrystalline Shape-Memory Alloy. {{Pseudoelastic}} Cycling Effects and Microstructural Modifications},
  author = {Sade, M. and family=Castro Bubani, given=F., prefix=de, useprefix=true and Lovey, F. C. and Torra, V.},
